-- ---
-- Table 'databse'
-- 
-- ---

DROP TABLE IF EXISTS `databse`;
		
CREATE TABLE `databse` (
  `id` INTEGER NULL AUTO_INCREMENT DEFAULT NULL,
  `GIG NO` INTEGER NULL AUTO_INCREMENT DEFAULT NULL,
  `Refrence/Part No.` INTEGER NULL AUTO_INCREMENT DEFAULT NULL,
  `Description` MEDIUMTEXT NULL AUTO_INCREMENT DEFAULT NULL,
  `Serial No. of tester/panel meter/gauge` INTEGER NULL AUTO_INCREMENT DEFAULT NULL,
  `Cal done by(13BRD/MATCAL)` INTEGER NULL AUTO_INCREMENT DEFAULT NULL,
  `Cal Due` INTEGER NULL AUTO_INCREMENT DEFAULT NULL,
  PRIMARY KEY (`id`)
);

